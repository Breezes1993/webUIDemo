*{
	padding: 0;
	margin: 0;
	box-sizing: border-box;
}

body{
	/*background: red;*/
}
html{
	/*font-size:1px;*/
}
html,body{
	width: 100%;
	height: 100%;
	overflow: hidden;
}
header{
	width:100%;
	height:0.9rem !important;
	text-align:center;
	line-height: 0.9rem;
	color:#D8D9DB;
	background-color: #323953;
	font-size:0.45rem;
	position:absolute;
	top:0;
	left:0;
}
footer{
	width:100%;
	background: #62ECB1;
	height:0.9rem;
	position:absolute;
	bottom:0;
	left:0;
}
main{
	width:100%;
	background:url("../img/bg.png");
	height:100%;
	padding-top:0.9rem;
}
.clearfix:before, .clearfix:after {
	/*清除浮动，最好最标准的写法*/
	content: "";
	display: table;
}

.clearfix:after {
	clear: both;
}

.swiper-pagination-bullet-active{
	background-color: #68CBAE !important;
}